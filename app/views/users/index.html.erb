<h1>Listing users</h1>

<table>
  <thead>
    <tr>
      <th>Show</th>
      <th>New</th>
      <th>Create</th>
      <th>Edit</th>
      <th>Update</th>
      <th>Destroy</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.show %></td>
        <td><%= user.new %></td>
        <td><%= user.create %></td>
        <td><%= user.edit %></td>
        <td><%= user.update %></td>
        <td><%= user.destroy %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
